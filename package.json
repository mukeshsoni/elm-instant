{
  "name": "elm-instant",
  "main": "./lib/elm-instant",
  "version": "0.8.1",
  "description": "Try your elm code from the editor. Provides a visual REPL and a preview pane",
  "keywords": [
    "elm",
    "preview",
    "instant",
    "playground"
  ],
  "activationCommands": {
    "atom-workspace": "elm-instant:toggle"
  },
  "scripts": {
    "lint": "standard lib",
    "install-elm-packages": "node elm-scripts",
    "preinstall": "npm run install-elm-packages"
  },
  "repository": "https://github.com/mukeshsoni/elm-instant",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-package-deps": "^4.3.1",
    "bluebird": "^3.4.6",
    "classnames": "2.2.5",
    "jsonfile": "2.4.0",
    "lodash": "4.15.0",
    "ps-node": "^0.1.2",
    "react-elm-components": "1.0.0",
    "react-for-atom": "15.3.1-0",
    "rimraf": "^2.5.4",
    "rxjs": "5.0.0-beta.12"
  },
  "devDependencies": {
    "rimraf": "^2.5.4"
  },
  "package-deps": [
    "language-elm",
    "elm-format"
  ]
}
